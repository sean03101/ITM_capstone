<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sample.FragmentAlarm">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar3" />


    <LinearLayout
    android:id="@+id/linearLayout2"
    android:layout_width="409dp"

    android:layout_height="46dp"
    android:layout_marginTop="10dp"
    android:orientation="horizontal"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
    tools:ignore="MissingConstraints">

    <Button
        android:id="@+id/button5"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:layout_weight="1"
        android:layout_margin="5dp"
        android:background="#8D9A84"
        android:theme="@style/PrimaryButton"
        android:fontFamily="@font/ibm"
        android:text="무역/유통" />

    <Button
        android:id="@+id/button6"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:layout_margin="5dp"
        android:background="#8D9A84"
        android:theme="@style/PrimaryButton"
        android:fontFamily="@font/ibm"
        android:layout_weight="1"
        android:text="영업/서비스" />

    <Button
        android:id="@+id/button7"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:layout_margin="5dp"
        android:background="#8D9A84"
        android:theme="@style/PrimaryButton"
        android:fontFamily="@font/ibm"
        android:layout_weight="1"
        android:text="연구개발/설계" />

    <Button
        android:id="@+id/button8"
        android:layout_width="0dp"
        android:layout_margin="5dp"
        android:background="#8D9A84"
        android:theme="@style/PrimaryButton"
        android:fontFamily="@font/ibm"
        android:layout_height="34dp"
        android:layout_weight="1"
        android:text="건설/토목" />
</LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="409dp"

        android:layout_height="46dp"
        android:layout_marginTop="30dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/button1"
            android:layout_width="0dp"
            android:layout_height="34dp"
            android:layout_margin="5dp"
            android:background="#8D9A84"
            android:fontFamily="@font/ibm"

            android:theme="@style/PrimaryButton"
            android:layout_weight="1"
            android:text="경영/사무" />

        <Button
            android:id="@+id/button2"
            android:layout_width="0dp"
            android:layout_margin="5dp"
            android:background="#8D9A84"
            android:theme="@style/PrimaryButton"
            android:fontFamily="@font/ibm"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="마케팅/홍보" />

        <Button
            android:id="@+id/button3"
            android:layout_margin="5dp"
            android:background="#8D9A84"
            android:theme="@style/PrimaryButton"
            android:fontFamily="@font/ibm"
            android:layout_width="0dp"

            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="IT/디지털" />

        <Button
            android:id="@+id/button4"
            android:layout_margin="5dp"
            android:background="#8D9A84"
            android:theme="@style/PrimaryButton"
            android:layout_width="0dp"

            android:fontFamily="@font/ibm"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="디자인" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/ScrollView1"

        android:layout_width="411dp"
        android:layout_height="464dp"

        android:layout_marginTop="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist2"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist3"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist4"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist5"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist6"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView_dashlist7"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />


        </LinearLayout>

    </ScrollView>

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="410dp"
        android:layout_height="51dp"

        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="480dp"
        android:layout_marginBottom="25dp"
        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-3940256099942544/6300978111"
        app:layout_constraintBottom_toTopOf="@+id/ScrollView1"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="1dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

