<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="?attr/selectableItemBackground"
    android:clickable="true"
    android:focusable="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/recyclerview_studybox"
        android:layout_width="414dp"
        android:layout_height="111dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints">


        <LinearLayout
            android:layout_width="364dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="40dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/recyclerview_studybox_high"
                android:layout_width="81dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/rr"
                android:fontFamily="@font/ibm"
                android:text="스터디제목"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/recyclerview_studybox_middle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:textStyle="italic"
                android:fontFamily="@font/ibm"
                android:text="스터디설명" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/recyclerview_studybox_low_1"
                    android:layout_width="119dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7dp"
                    android:fontFamily="@font/ibm"
                    android:text="회사명" />

                <TextView
                    android:id="@+id/recyclerview_studybox_low_4"
                    android:layout_width="238dp"
                    android:layout_height="wrap_content"

                    android:fontFamily="@font/ibm"
                    android:text="하위 카테고리" />

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>





</androidx.constraintlayout.widget.ConstraintLayout>